{
  "manifest_version": 3,
  "name": "Competitive programing problems manager",
  "version": "0.1",
  "description": "A chrome extension which can manage your competitive programing problem list",
  "background": {
    "service_worker": "./background.js"
  },
  "permissions": [
    "tabs",
    "storage",
    "history",
    "contextMenus"
  ],
  "author": "Wilson Lu",
  "homepage_url": "https://github.com/93wilsonlu/cp-problem-manager",
  "short_name": "React Extension",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Competitive programing problems manager",
    "default_icon": {
      "48": "/images/logo_48.png",
      "64": "/images/logo_64.png",
      "128": "/images/logo_128.png",
      "256": "/images/logo_256.png",
      "512": "/images/logo_512.png"
    }
  },
  "icons": {
    "48": "/images/logo_48.png",
    "64": "/images/logo_64.png",
    "128": "/images/logo_128.png",
    "256": "/images/logo_256.png",
    "512": "/images/logo_512.png"
  }
}